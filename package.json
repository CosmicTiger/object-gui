{
  "name": "objectgui",
  "version": "1.0.0-alpha.3",
  "description": "ObjectGUI - Fully customizable Javascript Object GUI Editor",
  "keywords": [
    "editor",
    "dat.gui",
    "javascript",
    "wyiswyg",
    "object",
    "gui",
    "objectgui"
  ],
  "author": "Matteo Bruni <ar3s@icloud.com>",
  "homepage": "https://editor.matteobruni.it/",
  "license": "ISC",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist/**/*"
  ],
  "main": "dist/js/index.js",
  "jsdelivr": "dist/js/objectgui.min.js",
  "unpkg": "dist/js/objectgui.min.js",
  "browser": "dist/js/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matteobruni/objectgui.git"
  },
  "scripts": {
    "prettify:src": "prettier --write ./src/",
    "prettify:readme": "prettier --write ./README.md",
    "lint": "eslint src/ts --ext .js,.jsx,.ts,.tsx --fix",
    "clear:dist": "rimraf -f ./dist",
    "build:ts:es": "tsc -b tsconfig.browser.json",
    "build:ts": "tsc",
    "build:css": "sass src/scss/editor.scss dist/css/objectgui.css",
    "build": "yarn clear:dist && yarn build:css && yarn prettify:src && yarn build:ts:es && yarn lint && webpack -p && yarn build:ts && yarn prettify:readme",
    "prepare": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/matteobruni/objectgui/issues"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "fibers": "^5.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "terser-webpack-plugin": "^3.0.7",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  }
}
